<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pds.dao.ActorMapper">
    <select id="getAllActors" resultType="com.pds.entity.Actor">
        select * from userinfo
    </select>

    <select id="getActorById" resultType="com.pds.entity.Actor" parameterType="int">
        select * from userinfo where username = #{username}
    </select>

</mapper>    